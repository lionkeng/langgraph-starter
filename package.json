{
  "name": "node-typescript-template",
  "version": "1.0.0",
  "description": "a simple nodeJS typescript template",
  "main": "main.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "build-swc": "npx swc src -d dist",
    "start": "node -r tsconfig-paths/register -r dotenv/config dist/main.js",
    "dev": "tsx --experimental-wasm-modules -r dotenv/config src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lionkeng/node-typescript-template.git"
  },
  "keywords": [
    "nodeJS",
    "typescript",
    "template"
  ],
  "author": "lionkeng@gmail.com",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@swc/helpers": "^0.5.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.9.0",
    "dotenv": "^16.3.1",
    "lerna": "^8.1.3",
    "regenerator-runtime": "^0.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.10.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@langchain/community": "^0.2.4",
    "@langchain/core": "^0.2.2",
    "@langchain/langgraph": "^0.0.21",
    "@langchain/openai": "^0.0.34",
    "langchain": "^0.2.3",
    "langsmith": "^0.1.30",
    "zod": "^3.23.8"
  }
}
